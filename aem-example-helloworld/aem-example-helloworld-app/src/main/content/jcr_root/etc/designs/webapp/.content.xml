<?xml version="1.0" encoding="UTF-8"?>
<jcr:root 
	xmlns:sling="http://sling.apache.org/jcr/sling/1.0" 
	xmlns:cq="http://www.day.com/jcr/cq/1.0" 
	xmlns:jcr="http://www.jcp.org/jcr/1.0" 
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/libs/wcm/core/templates/designpage"
        jcr:primaryType="cq:PageContent"
        jcr:title="webapp"
        sling:resourceType="wcm/core/components/designer">
        <inputRenderer jcr:primaryType="nt:unstructured">
        	<par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys"                
                components="[group:Hello World - Input,group:Hello World - Input Form]">
             </par>   
        </inputRenderer> 
        <form jcr:primaryType="nt:unstructured">
        	<formParsys
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aem-example-helloworld/components/parsys/formParsys"                
                components="[group:Hello World - Input Form]"/>
        </form> 
        <outputRenderer jcr:primaryType="nt:unstructured">
        	<par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys"                
                components="[group:Hello World - Output]"/>
        </outputRenderer>              
    </jcr:content>    
</jcr:root>