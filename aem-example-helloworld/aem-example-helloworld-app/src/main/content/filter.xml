<?xml version="1.0" encoding="UTF-8"?>
<!--
    Defines which repository items are generally included
-->
<workspaceFilter version="1.0">
    <filter root="/apps">
      <include pattern="/apps/aem-example-helloworld(/.*)?"/>
    </filter>

    <filter root="/etc/designs">
        <include pattern="/etc/designs/webapp(/.*)?"/>
    </filter>
</workspaceFilter>